<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
    <% if !current_user.eql? user%>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
         <%= render 'shared/friendships_links',user: user  %>
        </span>
        
      </li>
      <%end%>
    <% end %>
  </ul>
</section>

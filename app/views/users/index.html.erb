<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
    <% if !current_user.eql? user%>

      <div class="card w-50 shadow-lg my-4">
     <div class="card-body">
    <h5 class="card-title"> Name: <%= user.name%></h5>
    <%= link_to 'See Profile',  user_path(user), class: ' btn btn-primary' %>
     <%= render 'shared/friendships_links',user: user  %>
  
  </div>
  </div>

      <%end%>
    <% end %>
  </ul>
</section>
